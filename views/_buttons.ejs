<div class="btns-wrapper">
    <a href="/" class="logo">CodeSharing</a>
    <div class="buttons">
        <a href="/new" class="btn">New</a>
        <% if (locals.canSave) { %>
        <button class="btn" type="submit">Save</button>
        <% } %>


        <% if (locals.id != null) { %>
            <a href="/<%= locals.id %>/duplicate" class="btn">Duplicate</a>
        <% } %>
        <button class="btn" type="button" id="copy-btn">Copy</button>
    </div>
</div>